    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.0.3",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "jest": "^26.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
