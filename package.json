  "scripts": {
    "build": "rm -rf build && babel src --out-dir build --extensions \".ts,.tsx\"",
    "coverage": "jest --coverage",
    "emit": "tsc --declaration --outDir ./build --emitDeclarationOnly --declarationMap --allowJs false --checkJs false src/*",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "jest --forceExit --runInBand",
    "preversion": "yarn test && yarn lint",
    "version": "yarn build && git add -A build",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
